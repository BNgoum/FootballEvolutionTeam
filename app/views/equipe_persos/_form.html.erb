<%= simple_form_for(@equipe_perso) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :nameteam %>
    <%= f.input :nametrainer %>
    <%= f.input :pays %>
    <label>Image</label>
      <%= f.label :image %><br>
      <%= f.file_field :image %>
    </div>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
